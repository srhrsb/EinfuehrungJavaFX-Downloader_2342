<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="148.0" prefWidth="430.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.brh.einfuehrungjavafx.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <HBox prefHeight="23.0" prefWidth="390.0">
         <children>
            <Label prefHeight="26.0" prefWidth="146.0" text="URL:" />
            <TextField fx:id="urlTf" prefHeight="26.0" prefWidth="243.0" />
         </children>
      </HBox>
      <HBox layoutX="30.0" layoutY="30.0" prefHeight="23.0" prefWidth="390.0">
         <children>
            <Label prefHeight="25.0" prefWidth="158.0" text="Ordner:" />
            <TextField fx:id="directoryTf" prefHeight="26.0" prefWidth="233.0" />
            <Button mnemonicParsing="false" onAction="#searchDirectory" text="..." />
         </children>
      </HBox>
      <Button disable="true" mnemonicParsing="false" onAction="#download" prefHeight="26.0" prefWidth="416.0" text="Download" />
   </children>
</VBox>
